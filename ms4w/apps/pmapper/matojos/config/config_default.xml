<pmapper>
    <ini>
        <pmapper>
            <pmTitle>Matojos</pmTitle>
            <debugLevel>3</debugLevel>
            <plugins>export</plugins>
            <plugins>scalebar</plugins>
            <plugins>transparency</plugins>
            <plugins>proj4js</plugins>
        </pmapper>
        <config>
            <pm_config_location>default</pm_config_location>
            <pm_javascript_location>javascript</pm_javascript_location>
            <pm_print_configfile>common/print.xml</pm_print_configfile>
            <pm_search_configfile>inline</pm_search_configfile>
        </config>
        <map>
            <mapFile>er_planico_loco.map</mapFile>
            <tplMapFile>common/template.map</tplMapFile>
            <categories>
                <category name="Distritos municipales">
					<group>distritos</group>
                </category>
                <category name="Parques y jardines públicos">
					<group>jardines</group>
					<group>zonas</group>
					<group>albero</group>
					<group>masas_arbustos</group>
					<group>flores</group>
					<group>setos</group>
                </category>
                <category name="Flora urbana">
                    <group>arboles</group>
					<group>palmeras</group>
					<group>arbustos</group>
                </category>
            </categories>
            <allGroups>
				<group>distritos</group>
                <group>rivers</group>
				<group>arboles</group>
				<group>palmeras</group>
				<group>arbustos</group>
				<group>setos</group>
				<group>jardines</group>
				<group>zonas</group>
				<group>albero</group>
				<group>flores</group>
				<group>masas_arbustos</group>
            </allGroups>
            <defGroups>
				<group>distritos</group>
				<group>arboles</group>
				<group>palmeras</group>
				<group>arbustos</group>
				<group>setos</group>
				<group>jardines</group>
				<group>zonas</group>
				<group>albero</group>
				<group>flores</group>
				<group>masas_arbustos</group>
            </defGroups>
            <layerAutoRefresh>1</layerAutoRefresh>
            <imgFormat>png8</imgFormat>
            <altImgFormat>jpeg</altImgFormat>
            <!--
            <altImgFormatLayers>
                <layer>jpl_wms_global_mosaic</layer>
                <layer>dem</layer>
            </altImgFormatLayers>
            -->
            <sliderMax>max</sliderMax>
            <sliderMin>25</sliderMin>
        </map>
        <query>
            <limitResult>300</limitResult>
            <highlightColor>0 255 255</highlightColor>
            <highlightSelected>1</highlightSelected>
            <autoZoom>nquery</autoZoom>
            <autoZoom>search</autoZoom>
            <zoomAll>search</zoomAll>
            <zoomAll>nquery</zoomAll>
            <infoWin>dynwin</infoWin>
            <alignQueryResults>1</alignQueryResults>
            <pointBuffer>100</pointBuffer>
            <shapeQueryBuffer>0.02</shapeQueryBuffer>
        </query>
        <ui>
            <tocStyle>tree</tocStyle>
            <legendStyle>attached</legendStyle>
            <useCategories>1</useCategories>
            <catWithCheckbox>1</catWithCheckbox>
            <scaleLayers>1</scaleLayers>
            <icoW>18</icoW>
            <icoH>14</icoH> 
			<legendKeyimageRewrite>0</legendKeyimageRewrite>
        </ui>
        <locale>
            <defaultLanguage>es</defaultLanguage>
            <defaultCharset>UTF-8</defaultCharset>
            <map2unicode>1</map2unicode>
        </locale>
        <print>
            <printImgFormat>png</printImgFormat>
            <printAltImgFormat>jpeg</printAltImgFormat>
            <pdfres>2</pdfres>
        </print>
        <download>
            <dpiLevels>150</dpiLevels>
            <dpiLevels>200</dpiLevels>
            <dpiLevels>300</dpiLevels>
        </download>
        <php>
            <pearDbClass>MDB2</pearDbClass>
            <defaultTimeZone>Europe/Madrid</defaultTimeZone>
        </php>
        <pluginsConfig>
            <export>
                <formats>XLS</formats>
                <formats>CSV</formats>
                <formats>PDF</formats>
            </export>
        </pluginsConfig>
    </ini>
    
    
    
    <searchlist version="1.0">
        <dataroot>$</dataroot>
        <searchitem name="sugerir_jardines" description="Jardines">
            <layer type="ms" name="jardines">
                <field type="s" name="NOMB_JARD" description="Jardines" wildcard="2">
                    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
            </layer>
        </searchitem>
        <searchitem name="sugerir_arboles" description="Árboles">
            <layer type="ms" name="arboles">
                <field type="s" name="COMUN" description="Árboles Común" wildcard="2" operator="OR">
                    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
				<field type="s" name="CIENTIFICO" description="Árboles Científico" wildcard="2" operator="OR">
				    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
            </layer>
        </searchitem>
        <searchitem name="sugerir_palmeras" description="Palmeras">
            <layer type="ms" name="palmeras">
                <field type="s" name="COMUN" description="Palmeras Común" wildcard="2" operator="OR">
                    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
				<field type="s" name="CIENTIFICO" description="Palmeras Científico" wildcard="2" operator="OR">
				    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
            </layer>
        </searchitem>
        <searchitem name="sugerir_arbustos" description="Arbustos">
            <layer type="ms" name="arbustos">
                <field type="s" name="COMUN" description="Arbustos Común" wildcard="2" operator="OR">
                    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
				<field type="s" name="CIENTIFICO" description="Arbustos Científico" wildcard="2" operator="OR">
				    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
            </layer>
        </searchitem>
        <searchitem name="sugerir_flores" description="Flores">
            <layer type="ms" name="flores">
                <field type="s" name="FLOR_TEMP" description="Flores" wildcard="2">
                    <definition type="suggest" connectiontype="ms" minlength="1" sort="asc">
                        <mslayer encoding="ISO-8859-1"/>
                    </definition>
                </field>
            </layer>
        </searchitem>
    </searchlist>
	
</pmapper>